package projetogcm;

public class UtilitarioInutil {

    public static final String VERSAO_FERRAMENTA = "1.0.0";
    public static final String MENSAGEM_PADRAO = "Configuração inicial validada.";

    private UtilitarioInutil() {
    }

    public static String obterStatus(String nomeComponente) {
        return "Componente " + nomeComponente + " está OK. Versão: " + VERSAO_FERRAMENTA;
    }
}
